{
  "name": "Edupage Addons",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "whatever the description was",
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "*://*.edupage.org/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "src/injs/*"
  ]
}
